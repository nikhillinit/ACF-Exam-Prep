  "conceptualThemes": [
    {
      "id": "T1",
      "name": "Information vs Mechanics",
      "description": "M&M perfect markets vs real-world signaling effects",
      "applicableArchetypes": ["A2B", "A5"],
      "keyPrinciples": [
        "Mechanical equivalence under perfect information",
        "Information asymmetry creates value/cost",
        "Costly signaling separates types"
      ]
    },
    {
      "id": "T2",
      "name": "Risk vs Return",
      "description": "How leverage and hedging affect expected returns and volatility",
      "applicableArchetypes": ["A1", "A3", "A6"],
      "keyPrinciples": [
        "Higher leverage increases equity risk",
        "Hedging reduces volatility but may reduce returns",
        "Risk-return tradeoff in capital structure"
      ]
    },
    {
      "id": "T3",
      "name": "Value Creation vs Transfer",
      "description": "Distinguishing NPV creation from wealth transfers between claimants",
      "applicableArchetypes": ["A2A", "A2B", "A4"],
      "keyPrinciples": [
        "Positive NPV creates value for all",
        "Wealth transfers don't create value",
        "Zero-sum games in redistribution"
      ]
    },
    {
      "id": "T4",
      "name": "Incentive Conflicts",
      "description": "Equity-debt agency problems and risk-shifting",
      "applicableArchetypes": ["A2A", "A4"],
      "keyPrinciples": [
        "Equity has option-like payoffs",
        "Highly leveraged firms prefer volatility",
        "Debt covenants mitigate conflicts"
      ]
    },
    {
      "id": "T5",
      "name": "Economic vs Financial Distress",
      "description": "Distinguishing cash flow problems from capital structure issues",
      "applicableArchetypes": ["A1", "A4"],
      "keyPrinciples": [
        "Economic distress: bad projects/operations",
        "Financial distress: can't meet obligations",
        "Financial distress can be solved by restructuring"
      ]
    },
    {
      "id": "T6",
      "name": "Project Risk â‰  Firm Risk",
      "description": "Projects should be discounted at appropriate project-specific rates",
      "applicableArchetypes": ["A3"],
      "keyPrinciples": [
        "Use project beta, not firm beta",
        "Unlever firm beta if leverage differs",
        "Match discount rate to project risk"
      ]
    },
    {
      "id": "T7",
      "name": "Asymmetric Information Effects",
      "description": "How information gaps affect financing and investment decisions",
      "applicableArchetypes": ["A2B", "A5"],
      "keyPrinciples": [
        "Pecking order: internal > debt > equity",
        "Adverse selection in equity markets",
        "Signaling via costly actions"
      ]
    }
  ],
